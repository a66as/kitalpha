<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated metadata -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.polarsys.kitalpha.ad.metadata.metadata.presentation.MetadataModelWizardID"
            name="%_UI_MetadataModelWizard_label"
            class="org.polarsys.kitalpha.ad.metadata.metadata.presentation.MetadataModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/MetadataModelFile.gif">
         <description>%_UI_MetadataModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated metadata -->
      <editor
            id="org.polarsys.kitalpha.ad.metadata.metadata.presentation.MetadataEditorID"
            name="%_UI_MetadataEditor_label"
            icon="icons/full/obj16/MetadataModelFile.gif"
            extensions="afm"
            class="org.polarsys.kitalpha.ad.metadata.metadata.presentation.MetadataEditor"
            contributorClass="org.polarsys.kitalpha.ad.metadata.metadata.presentation.MetadataActionBarContributor">
      </editor>
   </extension>

</plugin>
