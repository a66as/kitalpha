    get-item "Additional Metadata" | get-property "values['Property']" | equals "Additional Metadata" | verify-true
    get-item "Additional Metadata" | get-property "values['Value']" | equals "Metadata _b666QRBmEeeuXe9hCjRYAw" 
        | verify-true
}
